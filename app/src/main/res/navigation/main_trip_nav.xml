<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_trip_nav"
    app:startDestination="@id/trip_HomePage">

    <fragment
        android:id="@+id/blogHomeFragment"
        android:name="com.example.tripper_android_app.blog.BlogHomeFragment"
        android:label="fragment_blog_home"
        tools:layout="@layout/fragment_blog_home" />
    <fragment
        android:id="@+id/blog_HomePage"
        android:name="com.example.tripper_android_app.blog.Blog_HomePage"
        android:label="fragment_blog__home_page"
        tools:layout="@layout/fragment_blog__home_page" >
        <action
            android:id="@+id/action_blog_HomePage_to_create_Blog_Location_List"
            app:destination="@id/create_Blog_Location_List" />
    </fragment>
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.tripper_android_app.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment_to_blogMainFragment"
            app:destination="@id/blogMainFragment" />
    </fragment>

    <fragment
        android:id="@+id/setting_Fragment"
        android:name="com.example.tripper_android_app.setting.Setting_Fragment"
        android:label="fragment_setting_"
        tools:layout="@layout/fragment_setting_" >
        <action
            android:id="@+id/action_setting_Fragment_to_register_main_Fragment"
            app:destination="@id/register_main_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action


            android:id="@+id/action_setting_Fragment_to_register_Member_Fragment"
            app:destination="@id/register_Member_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_setting_Fragment_to_locationListFragment"
            app:destination="@id/location_List_Fragment"
            />
        <action
            android:id="@+id/action_setting_Fragment_to_railwayTimeListFragment"
            app:destination="@id/railwayTimeListFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/register_main_Fragment"
        android:name="com.example.tripper_android_app.setting.member.Register_main_Fragment"
        android:label="fragment_register_main_"
        tools:layout="@layout/fragment_register_main_" >
        <action
            android:id="@+id/action_register_main_Fragment_to_register_NormalFragment"
            app:destination="@id/register_NormalFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_main_Fragment_to_register_Login_Fragment"
            app:destination="@id/register_Login_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_main_Fragment_to_FB_Login_Fragment"
            app:destination="@id/FB_Login_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_main_Fragment_to_google_Login_Fragment"
            app:destination="@id/google_Login_Fragment"
            app:exitAnim="@anim/slide_out_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_main_Fragment_to_setting_Fragment"
            app:destination="@id/setting_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_main_Fragment_to_register_Member_Fragment"
            app:destination="@id/register_Member_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/register_NormalFragment"
        android:name="com.example.tripper_android_app.setting.member.Register_Normal_Fragment"
        android:label="fragment_register__normal"
        tools:layout="@layout/fragment_register_normal" >
        <action
            android:id="@+id/action_register_NormalFragment_to_register_Member_Fragment"
            app:destination="@id/register_Member_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
    </fragment>
    <fragment
        android:id="@+id/register_Member_Fragment"
        android:name="com.example.tripper_android_app.setting.member.Register_Member_Fragment"
        android:label="fragment_register__member_"
        tools:layout="@layout/fragment_register__member_" >
        <action
            android:id="@+id/action_register_Member_Fragment_to_register_main_Fragment"
            app:destination="@id/register_main_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"/>
        <action
            android:id="@+id/action_register_Member_Fragment_to_setting_Fragment"
            app:destination="@id/setting_Fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/register_Login_Fragment"
        android:name="com.example.tripper_android_app.setting.member.Normal_Login_Fragment"
        android:label="fragment_register__login_"
        tools:layout="@layout/fragment_register__login_" >
        <action
            android:id="@+id/action_register_Login_Fragment_to_register_Member_Fragment"
            app:destination="@id/register_Member_Fragment"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/slide_out_right"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"/>
    </fragment>
    <fragment
        android:id="@+id/insert_Location_Fragment"
        android:name="com.example.tripper_android_app.location.LocationInsertFragment"
        android:label="fragment_location_insert"
        tools:layout="@layout/fragment_location_insert" />
    <fragment
        android:id="@+id/groupFragment"
        android:name="com.example.tripper_android_app.group.GroupFragment"
        android:label="fragment_group"
        tools:layout="@layout/fragment_group" />
    <fragment
        android:id="@+id/notifyFragment"
        android:name="com.example.tripper_android_app.notify.NotifyFragment"
        android:label="fragment_notify"
        tools:layout="@layout/fragment_notify" />
    <fragment
        android:id="@+id/location_List_Fragment"
        android:name="com.example.tripper_android_app.location.LocationListFragment"
        android:label="fragment_location_list"
        tools:layout="@layout/fragment_location_list" >
        <action
            android:id="@+id/action_locationListFragment_to_insertLocationFragment"
            app:destination="@id/insert_Location_Fragment"
            />
        <action
            android:id="@+id/action_location_List_Fragment_to_locationDetailFragment"
            app:destination="@id/location_Detail_Fragment" />
        <action
            android:id="@+id/action_location_List_Fragment_to_locationUpdateFragment"
            app:destination="@id/location_Update_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/FB_Login_Fragment"
        android:name="com.example.tripper_android_app.setting.member.FB_Login_Fragment"
        android:label="fragment_f_b__login_"
        tools:layout="@layout/fragment_f_b__login_" >
        <action
            android:id="@+id/action_FB_Login_Fragment_to_register_Member_Fragment"
            app:destination="@id/register_Member_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/google_Login_Fragment"
        android:name="com.example.tripper_android_app.setting.member.Google_Login_Fragment"
        android:label="fragment_google__login_"
        tools:layout="@layout/fragment_google__login_" />
    <fragment

        android:id="@+id/blogMainFragment"
        android:name="com.example.tripper_android_app.blog.BlogMainFragment"
        android:label="fragment_blog_main"
        tools:layout="@layout/fragment_blog_main" />
<fragment
        android:id="@+id/create_Trip_Fragment"
        android:name="com.example.tripper_android_app.trip.Create_Trip_Fragment"
        android:label="fragment_create__trip_"
        tools:layout="@layout/fragment_create__trip_" >
        <action
            android:id="@+id/action_create_Trip_Fragment_to_create_Trip_LocationLsit"
            app:destination="@id/create_Trip_LocationList" />
    </fragment>
    <fragment
        android:id="@+id/trip_HomePage"
        android:name="com.example.tripper_android_app.trip.Trip_HomePage"
        android:label="fragment_trip__home_page"
        tools:layout="@layout/fragment_trip__home_page" >
        <action
            android:id="@+id/action_trip_HomePage_to_create_Trip_Fragment"
            app:destination="@id/create_Trip_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/create_Trip_LocationList"
        android:name="com.example.tripper_android_app.trip.Create_Trip_LocationList"
        android:label="fragment_create_trip_location_lsit"
        tools:layout="@layout/fragment_create_trip_location_lsit" >
        <action
            android:id="@+id/action_create_Trip_LocationList_to_createTripLocationDetail"
            app:destination="@id/createTripLocationDetail" />
    </fragment>
    <fragment
        android:id="@+id/location_Detail_Fragment"
        android:name="com.example.tripper_android_app.location.LocationDetailFragment"
        android:label="LocationDetailFragment" />
    <fragment
        android:id="@+id/location_Update_Fragment"
        android:name="com.example.tripper_android_app.location.LocationUpdateFragment"
        android:label="LocationUpdateFragment" />
        <fragment
        android:id="@+id/createTripLocationDetail"
        android:name="com.example.tripper_android_app.trip.CreateTripLocationDetail"
        android:label="fragment_create_trip_location_detail"
        tools:layout="@layout/fragment_create_trip_location_detail" >
            <action
                android:id="@+id/action_createTripLocationDetail_to_createTripBeforeSave"
                app:destination="@id/createTripBeforeSave" />
        </fragment>
    <fragment
        android:id="@+id/createTripBeforeSave"
        android:name="com.example.tripper_android_app.trip.CreateTripBeforeSave"
        android:label="fragment_create_trip_before_save"
        tools:layout="@layout/fragment_create_trip_before_save" >
        <action
            android:id="@+id/action_createTripBeforeSave_to_createTripLocationDetail"
            app:destination="@id/createTripLocationDetail" />
    </fragment>

    <fragment
        android:id="@+id/create_Blog_Location_List"
        android:name="com.example.tripper_android_app.blog.Create_Blog_Location_List"
        android:label="fragment_create__blog__location__list"
        tools:layout="@layout/fragment_create_blog_trip_list" />
    <fragment
        android:id="@+id/railwayTimeListFragment"
        android:name="com.example.tripper_android_app.setting.railway.RailwayTimeListFragment"
        android:label="RailwayTimeListFragment" >
        <action
            android:id="@+id/action_railwayTimeListFragment_to_hsrTimeTableFragment"
            app:destination="@id/hsrTimeTableFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_railwayTimeListFragment_to_railwayTimeTableFragment"
            app:destination="@id/railwayTimeTableFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/hsrTimeTableFragment"
        android:name="com.example.tripper_android_app.setting.railway.HsrTimeTableFragment"
        android:label="fragment_hsr_time_table"
        tools:layout="@layout/fragment_hsr_time_table" />
    <fragment
        android:id="@+id/railwayTimeTableFragment"
        android:name="com.example.tripper_android_app.setting.railway.RailwayTimeTableFragment"
        android:label="fragment_railway_time_table"
        tools:layout="@layout/fragment_railway_time_table" />


</navigation>